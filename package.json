{
  "name": "freshell",
  "private": true,
  "version": "0.3.1",
  "type": "module",
  "scripts": {
    "predev": "tsx scripts/precheck.ts",
    "preserve": "tsx scripts/precheck.ts",
    "dev": "concurrently -n client,server -c blue,green \"vite\" \"tsx watch server/index.ts\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "typecheck": "npm run typecheck:client && npm run typecheck:server",
    "typecheck:client": "tsc -p tsconfig.json --noEmit",
    "typecheck:server": "tsc -p tsconfig.server.json --noEmit",
    "build": "npm run typecheck:client && npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "serve": "npm run build && npm run start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:server": "vitest --config vitest.server.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run --config vitest.server.config.ts test/server",
    "test:client": "vitest run test/unit/client",
    "test:all": "vitest run && vitest run --config vitest.server.config.ts",
    "prepare:icons": "node assets/icons/prepare-icons.mjs",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@monaco-editor/react": "^4.6.0",
    "@reduxjs/toolkit": "^2.3.0",
    "ai": "^4.3.19",
    "chokidar": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "glob": "^13.0.0",
    "lucide-react": "^0.469.0",
    "nanoid": "^5.0.7",
    "node-pty": "^1.2.0-beta.10",
    "pino": "^9.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-window": "^2.2.6",
    "remark-gfm": "^4.0.0",
    "rotating-file-stream": "^3.2.8",
    "ws": "^8.18.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-window": "^1.8.8",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^17.3.0",
    "jsdom": "^25.0.1",
    "pino-pretty": "^11.3.0",
    "postcss": "^8.4.49",
    "supertest": "^7.0.0",
    "superwstest": "^1.1.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  }
}
